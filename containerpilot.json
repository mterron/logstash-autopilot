{
  "consul": "{{ .CONSUL }}",
  "logging": {
    "level": "INFO",
    "format": "default",
    "output": "stdout"
  },
  "preStart": "/usr/local/bin/prestart.sh",
  "stopTimeout": 60,
  "services": [
    {
      "name": "syslog",
      "port": 3164,
      "poll": 3600,
      "ttl": 7200
    },
    {
      "name": "syslog-rfc5424",
      "port": 5424,
      "poll": 3600,
      "ttl": 7200
    },
    {
      "name": "relp",
      "port": 10514,
      "poll": 3600,
      "ttl": 7200
    },
    {
      "name": "gelf",
      "port": 12201,
      "poll": 3600,
      "ttl": 7200
    },
    {
      "name": "logstash-json_line",
      "port": 13000,
      "poll": 3600,
      "ttl": 7200
    },
    {
      "name": "logstash-json",
      "port": 14000,
      "poll": 3600,
      "ttl": 7200
    },
    {
      "name": "fluent",
      "port": 24224,
      "poll": 3600,
      "ttl": 7200
    },
    {
      "name": "beats",
      "port": 25109,
      "poll": 3600,
      "ttl": 7200
    },
    {
      "name": "logstash-nmap",
      "port": 6666,
      "poll": 3600,
      "ttl": 7200
    }
  ]
}
