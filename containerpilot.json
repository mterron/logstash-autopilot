{
  "consul": "{{ .CONSUL }}",
  "preStart": "/usr/local/bin/prestart.sh",
  "services": [
    {
      "name": "logstash",
      "port": 514,
      "health": "true",
      "poll": 10,
      "ttl": 25
    }
  ],
  "backends": [
    {
      "name": "elasticsearch",
      "poll": 5,
      "onChange": "/usr/local/bin/prestart.sh"
    }
  ]
}
